    "placeholder": "2001:db8:a0b:12f0::1",
    "description": {
      "en": "Match IPv6 addresses",
      "ru": "Соответствует IPv6 адресу"
    },
    "tags": "ip,ipv6,ip adress,adress,other",
    "nickname": "@Archakov06",
